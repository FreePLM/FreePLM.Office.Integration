<Window x:Class="FreePLM.Office.Integration.Windows.CheckInDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FreePLM.Office.Integration.Windows"
        xmlns:controls="clr-namespace:FreePLM.WPF.UserControls.UserControls;assembly=FreePLM.WPF.UserControls"
        mc:Ignorable="d"
        Title="Check In Document - FreePLM"
        Height="550"
        Width="500"
        WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize"
        ShowInTaskbar="True"
        Topmost="True">
    <Grid>
        <controls:CheckInUserControl x:Name="CheckInControl"/>
    </Grid>
</Window>
